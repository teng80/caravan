package Chapter14;

import java.util.Date;

public class KimTaeHyung_0504 {

	public static void main(String[] args) {

		Date date = new Date();
		long time = date.getHours();
		long time = date.getMinutes();
		  
		System.out.println("현재 시간은 " + time);
		if (time<11) {
			System.out.println("아침");
		}
		else if (time<15)
		System.out.println("점심");
		else if (time<20)
		System.out.println("오후");
		else
		System.out.println("밤");
		

	}

}
