package Chapter04;

public class ArrayParameterEx {

	static void printCharArry(char a[]) {
//		매개변수 크기만큼 반복
		for (int i = 0; i < a.length; i++) {
			System.out.print(a[i]);
		}
		System.out.println();
	}
	public static void main(String[] args) {
		char c[]= {'T','h','i','s',' ','i','s',' ','a',' ','p','e','n','c','i','l','.'};
		printCharArry(c);

	}

}
